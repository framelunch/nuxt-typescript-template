{
  "name": "nuxt-ts",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "axios": "^0.18.0",
    "nuxt": "^1.3.0",
    "nuxt-property-decorator": "^1.0.0",
    "sanitize.css": "^5.0.0",
    "vuex-class": "^0.3.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "dev": "nuxt",
    "prebuild": "npm run clean",
    "build": "nuxt build",
    "start": "nuxt start",
    "pregenerate": "npm run clean",
    "generate": "nuxt generate",
    "clean": "rimraf dist",
    "lint": "run-p lint:*",
    "lint:style": "stylelint './src/**/*.css'",
    "lint:script": "tslint './src/**/*.ts'",
    "lint:vue": "run-p lint:vue:*",
    "lint:vue:style": "stylelint --config .vue.stylelintrc './src/**/*.vue'",
    "fix": "prettier --write './src/**/*.{ts,js,json,css,vue}'"
  },
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.ts": [
        "prettier --write",
        "tslint",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "stylelint",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ],
      "*.vue": [
        "prettier --write",
        "stylelint --config .vue.stylelintrc",
        "git add"
      ]
    }
  },
  "browserslist": [
    "ie 11",
    "ios >= 10",
    "> 1% in JP",
    "not ie <= 10",
    "not chrome 49"
  ],
  "devDependencies": {
    "@types/node": "^9.4.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-nested": "^3.0.0",
    "postcss-url": "^7.3.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.1.1",
    "stylelint-config-framelunch": "framelunch/stylelint-config-framelunch#v0.3.0",
    "stylelint-processor-html": "^1.0.0",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2"
  }
}
